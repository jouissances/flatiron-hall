<div class="project">
  <p>Project Title: <%= @project.title %> </p>
  <div class="video">
    <iframe src="<%= @project.video_url %>" width="640" height="400" frameborder="0" allowfullscreen></iframe>
  </div>
  <p>Category: <%= @project.category %> </p>
  <p>Description: <%= @project.description %> </p>
  <p>GitHub Repo: <a href="<%= @project.github %>"><%= @project.github %></a> </p>
  <p>Project URL: <a href="<%= @project.external_uri %>"><%= @project.external_uri %></a> </p>
  <p>Made by <%= @project.user.username %> </p>
</div>

<form method="POST" action="/projects/<%= @project.id %>/delete">

  <input name="_method" type="hidden" value="DELETE">
  <input type="submit" id="submit" value="Delete Project">

</form>

<a href="/projects/<%= @project.id %>/edit">Edit Project </a>
